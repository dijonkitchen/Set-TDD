{
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.2.2",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "./node_modules/typescript/bin/tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
