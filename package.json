{
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@types/jest": "^22.1.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.2.2",
    "jest": "^22.3.0",
    "regenerator-runtime": "^0.11.1",
    "ts-jest": "^22.0.4",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "./node_modules/typescript/bin/tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
